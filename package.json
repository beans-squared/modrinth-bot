{
  "name": "modrunner",
  "version": "1.3.2",
  "private": true,
  "main": "index.js",
  "scripts": {
    "setup-workspace": "npm install && scoop install doppler && doppler login && doppler setup",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "eslint": "eslint .",
    "eslint-fix": "eslint fix .",
    "notused": "npm run register-globally && npm run alter-db",
    "start": "doppler run --command=\"node .\"",
    "dev": "doppler run --command=\"nodemon .\"",
    "register": "doppler run --command=\"node scripts/register-commands.js\"",
    "register-globally": "doppler run --command=\"node scripts/register-commands.js --global\"",
    "create-db": "node scripts/initialize-database.js",
    "reset-db": "node scripts/initialize-database.js --force",
    "alter-db": "node scripts/initialize-database.js --alter",
    "migrate-db": "doppler run --command=\"node scripts/migrate-database.js\"",
    "set-guild-max": "node scripts/set-guild-max-tracking.js"
  },
  "dependencies": {
    "@discordjs/rest": "^1.2.0",
    "dayjs": "^1.11.5",
    "discord-api-types": "^0.37.11",
    "discord.js": "^14.7.1",
    "ms": "^2.1.3",
    "pino": "^8.6.1",
    "pino-pretty": "^9.1.0",
    "sequelize": "^6.29.0",
    "sqlite3": "^5.1.1",
    "undici": "^5.10.0"
  },
  "devDependencies": {
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1"
  }
}
